{
  "name": "rkowert.com",
  "private": true,
  "description": "Home page Dr. Rachel Kowert containing information on her research, publications, and current projects",
  "version": "0.1.0",
  "author": "Dr. Rachel Kowert <rkowert@gmail.com>",
  "homepage": "https://rkowert.com",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rkowert/rkowert.com"
  },
  "bugs": {
    "url": "https://github.com/rkowert/rkowert.com/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "depcheck": "yarn depcheck",
    "deploy": "yarn build && cp now.json public/ && cd public && now alias $(now) rkowert.com",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "dry": "yarn build && cp now.json public/ && cd public && now",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{css,js,json,ts,tsx}}'",
    "lint": "./node_modules/.bin/eslint --ext .js,.ts,.tsx --ignore-pattern public .",
    "new": "ts-node ./scripts/new.ts",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "@devexpress/dx-react-core": "^1.10.5",
    "@devexpress/dx-react-grid": "^1.10.5",
    "@devexpress/dx-react-grid-material-ui": "^1.10.5",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@mdx-js/mdx": "^1.0.19",
    "@mdx-js/react": "^1.0.16",
    "@svgr/webpack": "^4.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gatsby": "^2.1.31",
    "gatsby-image": "^2.0.33",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-netlify-cms": "^4.0.1",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sharp": "^2.2.12",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-tags": "^2.3.0",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.24",
    "gatsby-transformer-remark": "^2.3.5",
    "gatsby-transformer-sharp": "^2.1.17",
    "inquirer": "^6.2.2",
    "linkifyjs": "^2.1.8",
    "moment": "^2.24.0",
    "netlify-cms-app": "^2.9.1",
    "polished": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-data-table-component": "^1.4.6",
    "react-dom": "^16.8.4",
    "react-headroom": "^2.2.8",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.5.0",
    "react-switch": "^5.0.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.2.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.5.0-rc",
    "typography": "^0.16.19",
    "typography-theme-us-web-design-standards": "^0.16.19",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@types/inquirer": "^6.0.0",
    "@types/moment": "^2.13.0",
    "@types/react-headroom": "^2.2.1",
    "@types/react-helmet": "^5.0.8",
    "@types/react-tagcloud": "^1.1.4",
    "@types/styled-components": "^4.1.12",
    "@types/typography": "^0.16.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "depcheck": "^0.8.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-eslint": "^2.0.4",
    "prettier": "^1.16.4",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.6.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": "error"
    }
  }
}
