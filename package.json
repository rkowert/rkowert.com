{
  "name": "rkowert.com",
  "private": true,
  "description": "Home page Dr. Rachel Kowert containing information on her research, publications, and current projects",
  "version": "1.0.0",
  "author": "Dr. Rachel Kowert <rkowert@gmail.com>",
  "homepage": "https://rkowert.com",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rkowert/rkowert.com"
  },
  "bugs": {
    "url": "https://github.com/rkowert/rkowert.com/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "depcheck": "yarn depcheck",
    "deploy": "yarn build && cp now.json public/ && cd public && now alias $(now) rkowert.com",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "dry": "yarn build && cp now.json public/ && cd public && now",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{css,js,json,ts,tsx}}'",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "./node_modules/.bin/eslint --ext .js,.ts,.tsx --ignore-pattern public .",
    "new": "ts-node ./scripts/new.ts",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "@devexpress/dx-react-core": "^2.7.6",
    "@devexpress/dx-react-grid": "^2.7.6",
    "@devexpress/dx-react-grid-material-ui": "^2.7.6",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.5.1",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@svgr/webpack": "^5.4.0",
    "babel-plugin-styled-components": "^1.13.2",
    "gatsby": "^3.13.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^3.13.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-mdx": "^2.13.0",
    "gatsby-plugin-netlify-cms": "^5.13.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-sitemap": "^4.9.0",
    "gatsby-plugin-styled-components": "^4.13.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "gatsby-plugin-tags": "^2.3.7",
    "gatsby-plugin-typescript": "^3.13.0",
    "gatsby-plugin-typography": "^3.13.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-autolink-headers": "^4.10.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^5.10.0",
    "gatsby-remark-responsive-iframe": "^4.10.0",
    "gatsby-remark-smartypants": "^4.10.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-transformer-json": "^3.13.0",
    "gatsby-transformer-remark": "^4.10.0",
    "gatsby-transformer-remark-frontmatter": "^1.1.0",
    "gatsby-transformer-sharp": "^3.13.0",
    "graphql": "^15.5.3",
    "inquirer": "^8.1.2",
    "linkifyjs": "^2.1.8",
    "moment": "^2.24.0",
    "netlify-cms-app": "^2.15.41",
    "polished": "^4.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-headroom": "^3.1.1",
    "react-helmet": "^6.0.0",
    "react-icons": "^4.2.0",
    "react-switch": "^6.0.0",
    "react-typography": "^0.16.20",
    "slug": "^5.1.0",
    "styled-components": "^5.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "typography": "^0.16.21",
    "typography-theme-us-web-design-standards": "^0.16.19",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-document-nodes": "2.1.4",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@types/inquirer": "^8.1.1",
    "@types/moment": "^2.13.0",
    "@types/react-headroom": "^2.2.2",
    "@types/react-helmet": "^6.1.2",
    "@types/react-tagcloud": "^1.1.6",
    "@types/styled-components": "^5.1.14",
    "@types/typography": "^0.16.4",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "depcheck": "^1.4.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "gatsby-plugin-eslint": "^4.0.0",
    "prettier": "^2.4.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "scripts/**/*",
      "src/types/graphql.ts"
    ],
    "extends": [
      "airbnb-base",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "import",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ],
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "no-use-before-define": "off",
      "quotes": [
        "error",
        "single"
      ],
      "semi": "error"
    },
    "settings": {
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "typescript": {
          "alwaysTryTypes": true
        }
      }
    }
  }
}
